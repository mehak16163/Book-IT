<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CRBSystem.FacultyIntro.Fac_cancelController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="New Booking">
               <font>
                  <Font name="Comic Sans MS" size="45.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="33.0" prefWidth="800.0">
         <children>
            <Label fx:id="confirm" text="Booking Confirmed" visible="false">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Label fx:id="warning" text="Enter Correct Details" textFill="#f80e0e" visible="false">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="22.0" prefWidth="800.0" spacing="70.0">
         <children>
            <Label alignment="CENTER" prefHeight="31.0" prefWidth="248.0" text="Name">
               <font>
                  <Font name="Comic Sans MS" size="21.0" />
               </font>
            </Label>
            <TextField fx:id="name" prefHeight="31.0" prefWidth="238.0" promptText="Enter Name" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0" stylesheets="@../../../Downloads/Classroom-Booking-System-master/stylesheets/Hbox.css">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="379.0" text="Time Slot">
               <font>
                  <Font name="Comic Sans MS" size="22.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <TextField alignment="CENTER" prefHeight="31.0" prefWidth="61.0" text="From">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <TextField fx:id="from" prefHeight="31.0" prefWidth="48.0" />
            <TextField alignment="CENTER" prefHeight="27.0" prefWidth="44.0" text="To">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="to" prefHeight="31.0" prefWidth="69.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="57.0" prefWidth="800.0">
         <children>
            <Label prefHeight="37.0" prefWidth="191.0" text="BookingDate">
               <font>
                  <Font name="Comic Sans MS" size="22.0" />
               </font>
            </Label>
            <DatePicker fx:id="date" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="58.0" prefWidth="600.0" spacing="70.0" stylesheets="@../../../Downloads/Classroom-Booking-System-master/stylesheets/Hbox.css">
         <children>
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="109.0" text="Room">
               <font>
                  <Font name="Comic Sans MS" size="22.0" />
               </font>
               <HBox.margin>
                  <Insets left="100.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="room" prefHeight="27.0" prefWidth="175.0" promptText="Enter room to be booked">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="600.0" spacing="75.0">
         <children>
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="124.0" text="Email ID">
               <font>
                  <Font name="Comic Sans MS" size="22.0" />
               </font>
               <HBox.margin>
                  <Insets left="100.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="eid" promptText="Enter email id">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="62.0" prefWidth="600.0" spacing="70.0">
         <children>
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="229.0" text="Capacity" textOverrun="CLIP">
               <font>
                  <Font name="Comic Sans MS" size="22.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <TextField fx:id="capacity" promptText="Enter Capacity">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="78.0" prefWidth="600.0" spacing="75.0">
         <children>
            <Button alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#back" prefHeight="68.0" prefWidth="176.0" stylesheets="@../../../stylesheets/button.css" text="&lt;&lt;Back">
               <font>
                  <Font name="Comic Sans MS" size="19.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#reset" stylesheets="@../../../stylesheets/button.css" text="Reset">
               <font>
                  <Font name="Comic Sans MS" size="19.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#submit" stylesheets="@../../../stylesheets/button.css" text="Submit">
               <font>
                  <Font name="Comic Sans MS" size="19.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#log" stylesheets="@../../../stylesheets/button.css" text="Logout" />
         </children>
      </HBox>
   </children>
</VBox>
